{
	"$schema": "https://biomejs.dev/schemas/2.3.13/schema.json",
	"extends": ["@gm-dev/biome/node"],
	"files": {
		"includes": ["./src/**/*.{ts}", "./prisma/**/*.{ts}", "./config/**/*.{ts}", "./prisma.config.ts"]
	},
	"linter": {
		"rules": {
			"suspicious": {
				"noExplicitAny": "off"
			},
			"complexity": {
				"noStaticOnlyClass": "off"
			},
			"style": {
				"noDefaultExport": "off"
			}
		}
	}
}
